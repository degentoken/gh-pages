git filter-branch --commit-filter '
        if [ "$GIT_COMMITTER_NAME" = "patrickdos94500@gmail.com" ];
        then
                GIT_COMMITTER_NAME="DegenDEV";
                GIT_AUTHOR_NAME="DegenDEV";
                GIT_COMMITTER_EMAIL="degentoken.finance@gmail.com";
                GIT_AUTHOR_EMAIL="degentoken.finance@gmail.com";
                git commit-tree "$@";
        else
                git commit-tree "$@";
        fi' HEAD
